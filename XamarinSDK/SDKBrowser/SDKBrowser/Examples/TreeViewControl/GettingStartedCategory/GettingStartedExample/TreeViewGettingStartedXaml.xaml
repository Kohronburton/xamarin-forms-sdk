<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:telerikDataControls="clr-namespace:Telerik.XamarinForms.DataControls;assembly=Telerik.XamarinForms.DataControls"
             xmlns:local="clr-namespace:SDKBrowser.Examples.TreeViewControl.GettingStartedCategory.GettingStartedExample"
             x:Class="SDKBrowser.Examples.TreeViewControl.GettingStartedCategory.GettingStartedExample.TreeViewGettingStartedXaml">
    <Grid>
        <!-- >> treeview-getting-started-xaml -->
        <telerikDataControls:RadTreeView x:Name="treeView" ItemsSource="{Binding Source}">
            <telerikDataControls:TreeViewDescriptor DisplayMemberPath="Name"
                                                    ItemsSourcePath="Children"
                                                    TargetType="{x:Type local:Item}" />
        </telerikDataControls:RadTreeView>
        <!-- << treeview-getting-started-xaml -->
    </Grid>
</ContentView>